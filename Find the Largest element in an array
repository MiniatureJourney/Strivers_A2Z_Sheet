import java.util.Scanner;

class Main {
    public static void main(String[] args) {
 Scanner obj = new Scanner(System.in);
 int max=8;
 int[] arr=new int[max];
 System.out.println("Enter the array ");
 for(int i=0;i<max;i++)
 {
     arr[i]=obj.nextInt();
 }
 int largest=0;
 for(int i=0;i<max;i++)
 {
     if(arr[i]>largest)
     {
         largest=arr[i];
     }
 }
 System.out.println(" The largest element in the array is "+ largest);
    }
}
