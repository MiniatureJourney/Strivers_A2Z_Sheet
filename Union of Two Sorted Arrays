import java.util.HashSet;
import java.util.Arrays;

class Main {
    public static void main(String[] args) {
        int[] arr1 = {1, 2, 2, 4, 5};
        int[] arr2 = {3, 4, 5, 6, 7};
Arrays.sort(arr1);
Arrays.sort(arr2);
HashSet<Integer> obj = new HashSet<>();
   
   for(int num:arr1)
   {
       obj.add(num);
   }
    for(int num:arr2)
   {
       obj.add(num);
   }
   
    System.out.println("Union of arrays:");
        System.out.println(obj);
    }
}
