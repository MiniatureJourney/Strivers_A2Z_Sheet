class Solution {

    public int missingNumber(int[] a, int N) {
        int expectedSum = N * (N + 1) / 2;
        int actualSum = 0;

        for (int num : a) {
            actualSum += num;
        }

        return expectedSum - actualSum;
    }

    public static void main(String[] args) {
        int[] a = {1, 2, 4, 5};
        int N = 5;

        System.out.println("The missing number is: " + new Solution().missingNumber(a, N));
    }
}
